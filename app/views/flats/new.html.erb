<%= simple_form_for @flat do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :address %>
  <%= f.input :surface_land %>
  <%= f.input :surface_habitation %>
  <%= f.input :surface_carrez %>
  <%= f.input :common_wall %>
  <%= f.input :rooms %>
  <%= f.input :bedroom %>
  <%= f.input :bathroom %>
  <%= f.input :year_construction %>
  <%= f.input :photos, as: :attachinary %>
  <%= f.button :submit %>
<% end %>


